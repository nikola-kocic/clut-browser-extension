{
  "manifest_version": 2,

  "name": "CLUT: Cycle Last Used Tabs",
  "description": "Cycle through last used tabs using keyboard shortcut.",
  "version": "1.6",
  "short_name": "CLUT",
  "applications": {
    "gecko": {
      "id": "{15678332-f6bc-45bc-96b6-c2787416c9c2}",
      "strict_min_version": "42.0"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background":{
    "scripts" : ["background.js"]
  },
  "commands": {
    "alt_switch_fast": {
      "suggested_key": {
        "default": "Alt+X"
      },
      "description": "Quick switch - Use to switch quickly among last 2 to 4 tabs. (Suggested: Alt+X)"
    },
    "alt_switch_slow_backward": {
      "suggested_key": {
        "default": "Alt+Z"
      },
      "description": "Normal switch backward (Suggested: Alt+Z) "
    },
    "alt_switch_slow_forward": {
      "suggested_key": {
        "default": "Alt+Shift+Z"
      },
      "description": "Normal switch forward (Suggested: Alt+Shift+Z)"
    }
  },
  "browser_action": {
    "default_icon": "icons/icon16.png"
  }
}
